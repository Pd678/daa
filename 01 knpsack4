# Dynamic Programming based Python Program for 0-1 Knapsack problem
# Returns the maximum value that can be put in a knapsack of capacity W

def knapSack(W, wt, val, n):
    dp = [0 for _ in range(W + 1)]  # DP array initialization

    for i in range(1, n + 1):  # Loop through items
        for w in range(W, 0, -1):  # Loop backwards through weights
            if wt[i - 1] <= w:  # Item fits
                dp[w] = max(dp[w], dp[w - wt[i - 1]] + val[i - 1])

    return dp[W]  # Max value with full capacity


# Driver code
val = [60, 100, 120]
wt = [10, 20, 30]
W = 50
n = len(val)

print(knapSack(W, wt, val, n))  # Output: 220
